<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.scantestdemo">

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-permission
        android:name="READ_PHONE_STATE"
        android:maxSdkVersion="29" />
    <uses-permission android:name="READ_PHONE_NUMBERS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <!-- 源码地址 ： https://github.com/journeyapps/zxing-android-embedded-->

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ScanTestDemo">
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CaptureActivity" />
        <activity android:name=".TestActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".CaptureAct" />
        <!--      生成二维码  -->
        <activity android:name=".CreateCodeActivity" />
        <!--      自定义选项  -->
        <!--        <activity android:name=".ExhibitScan" />-->

    </application>

</manifest>